{
    "name": "cwsps154/filament-notes-and-announcement",
    "description": "A filament plugin for save frontend customer notes and announcements",
    "version": "0.0.1",
    "keywords": [
        "cwsps154",
        "laravel",
        "filament",
        "filament-packages",
        "filament-plugins",
        "filament-notes-and-anouncements"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CWSPS154\\FilamentNotesAndAnnouncement\\": "src/",
            "CWSPS154\\FilamentNotesAndAnnouncement\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "authors": [
        {
            "name": "CWSPS154",
            "email": "codewithsps154@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CWSPS154\\FilamentNotesAndAnnouncement\\FilamentNotesAndAnnouncementServiceProvider"
            ]
        }
    }
}
